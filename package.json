{
  "name": "be-velora",
  "version": "1.0.0",
  "description": "Backend API for Velora - Maternal Health Application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'Build completed'",
    "test": "jest",
    "test:api": "node test-api.js",
    "test:simple": "node simple-test.js",
    "test:quick": "node quick-test.js",
    "test:comprehensive": "node comprehensive-test.js",
    "test:production": "API_URL=https://your-api-domain.vercel.app node comprehensive-test.js",
    "test:deployment": "node test-deployment.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "vercel-build": "echo 'Vercel build completed'",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "setup-env": "chmod +x setup-env.sh && ./setup-env.sh"
  },
  "keywords": [
    "maternal",
    "health",
    "pregnancy",
    "express",
    "supabase"
  ],
  "author": "Velora Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.43.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "nodemailer": "^6.9.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
